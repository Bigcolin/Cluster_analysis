    theta = qgplsim.theta_estimator(alpha, Bz)
    # f = open("theta.txt", "a")
    # println(θ, ": ", alpha, theta, "\n")
    # println(f, θ, ": ",alpha, theta, "\n")
    # close(f)
    alpha, theta
end
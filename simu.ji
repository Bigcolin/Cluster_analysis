include("model.ji")
import Base.Threads.@threads

Θ  = [[1,1], [2,2], [3,3], [4,4]]
α = [-1, 1]
# θ = [2, 2]
γ = [0, 0]
β = [0, -0]
# γ = [1, 1]
# β = [1, -0.5]
f = open("theta.txt", "w")
close(f)
nSim = 100
nSample = 200
@threads for θ in Θ
    Alpha = zeros(nSim, 2)
    Theta = zeros(nSim, 2)
    for i in 1:100
        Alpha[i,:], Theta[i,:] = main(γ, θ, α, β, nSample)
    end
    println(θ, ": Alpha =>", sum(Alpha, dims = 1), "Theta => ", sum(Theta, dims = 1))
end
